    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Server",
            "program": "${workspaceFolder}/src/server",
            "args": [],
            "cwd": "${workspaceFolder}/src"
        }, {
            "type": "lldb",
            "request": "launch",
            "name": "Client",
            "program": "${workspaceFolder}/src/client",
            "args": [],
            "cwd": "${workspaceFolder}/src"
        }, {
            "type": "lldb",
            "request": "launch",
            "name": "Generator",
            "program": "${workspaceFolder}/generator",
            "args": [
                "--puts", "100",
                "--gets", "50",
                "--ranges", "0",
                "--deletes", "0",
                "--gets-misses-ratio", "0.5",
                "--gets-skewness", "0.2"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}